<h1>Contact Us</h1>
<div class="container">
  <div class="row">
    <div class="col-md-6">
      <div class="jumbotron">
        <h3>Email Us</h3>
        <div class="form-group">
          <%= label :name %>
          <%= text_area :name %>
        </div>
        <div class="form-group">
          <%= label :phone_number %>
          <%= text_area :phone_number %>
        </div>
        <div class="form-group">
          <%= label :email %>
          <%= text_area :email %>
        </div>
        <div class="form-group">
          <%= label :message %>
          <%= text_field :message %>
        </div>
      </div>
    </div>
    <div class="col-md-6">
      <div class="jumbotron">
        <h3>Call Us</h3>
        <p><%= number_to_phone "801-819-5154" %></p>
      </div>
    </div>
  </div>
</div>
